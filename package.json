{
  "name": "@smirnovanv/links",
  "version": "3.0.2",
  "description": "Downloads files via links",
  "main": "bin/dist/index.js",
  "keywords": [
    "links",
    "npm",
    "package",
    "smirnovanv"
  ],
  "dependencies": {
    "eslint-plugin-import": "^2.25.2",
    "jest-junit": "^13.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.4.12",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.0.1",
    "husky": "^7.0.0",
    "jest": "^27.3.1",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.20",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "build": "npx tsc",
    "start": "node bin/dist/index.js",
    "lint": "eslint . --ext .ts",
    "test": "jest --coverage",
    "prepare": "husky install",
    "coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint . --ext .ts",
      "pre-push": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g './commitlint.js'"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./commitizen.js"
    }
  },
  "author": "",
  "license": "MIT",
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.spec.ts"
    ],
    "all": true,
    "check-coverage": true,
    "extension": [
      ".ts"
    ]
  },
  "bin": {
    "download-links": "./bin/dist/index.js"
  },
  "preferGlobal": true
}
